name: shell commands

on:
    push:
      branches:
        - feature/cicd

jobs:
    run-shell-commands:
        runs-on: ubuntu-latest
        steps:
            - name: string
              run: echo "welcome to githubactions cicd"
            - name: multiscripts
              run: |
                node -v
                npm -v
                echo "welcome to githubactions cicd"